<%= link_to 'Home', root_path %>
<% unless logged_in? %>
  <%= link_to 'Sign Up', sign_up_path %>
  <%= link_to 'Log in', login_path %>
<% else %>
  <span>Logged in as: <%= current_user.name.capitalize %></span>
  <%= link_to 'Log out', logout_path %>
  <%= link_to 'Create an event', new_event_path %>
  <%= link_to 'View my events', user_path(current_user) %>
<% end %>
<%= link_to 'View all events', events_path %>
